{
  "name": "tests-render-react-typescript",
  "version": "1.5.1",
  "description": "",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/js/*",
    "dev": "npm-run-all --parallel dev:watch dev:serve",
    "dev:serve": "ENTRY=./src/index.tsx ../../node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ../../scripts/bundling/webpack.config.dev.js --disableHostCheck true --port 3310 --host 0.0.0.0",
    "dev:watch": "ENTRY=./src/index.tsx ../../node_modules/webpack/bin/webpack.js --watch --config ../../scripts/bundling/webpack.config.dev.js",
    "webpack": "ENTRY=./src/index.tsx ../../node_modules/webpack/bin/webpack.js --config ../../scripts/bundling/webpack.config.prod.js",
    "build": "npm run clean && npm run webpack",
    "serve": "http-server -p 5000 dist",
    "typecheck": "../../node_modules/typescript/bin/tsc --noEmit --strict --jsx react --esModuleInterop src/index.tsx"
  },
  "dependencies": {
    "polythene-css": "^1.5.1",
    "polythene-react": "^1.5.1",
    "polythene-utilities": "^1.5.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.2.4",
    "typings": "^2.1.1"
  }
}
