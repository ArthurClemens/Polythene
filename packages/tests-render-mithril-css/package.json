{
  "name": "polythene-tests-render-mithril-css",
  "version": "1.3.2",
  "description": "",
  "private": true,
  "main": "dist/js/test.js",
  "module": "dist/js/test.mjs",
  "scripts": {
    "dev": "npm-run-all --parallel dev:watch dev:serve",
    "dev:serve": "../../node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ../../scripts/bundling/webpack.config.dev.js --port 3600 --host 0.0.0.0",
    "dev:watch": "../../node_modules/webpack/bin/webpack.js --watch --config ../../scripts/bundling/webpack.config.dev.js",
    "writeCSS": "node ./scripts/writeThemeCSS",
    "stats": "../../node_modules/webpack/bin/webpack.js --config ../../scripts/bundling/webpack.config.prod.js --profile --json > stats.json",
    "build": "npm run writeCSS && webpack --config ../../scripts/bundling/webpack.config.prod.js"
  },
  "dependencies": {
    "mithril": "2.0.0-rc.3",
    "polythene-css": "^1.3.2",
    "polythene-mithril": "^1.3.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "polythene-scripts": "^1.3.2"
  }
}
