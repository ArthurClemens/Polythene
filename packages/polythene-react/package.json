{
  "name": "polythene-react",
  "version": "1.4.1",
  "description": "",
  "main": "dist/polythene-react",
  "module": "dist/polythene-react.mjs",
  "scripts": {
    "clean": "rm -rf dist/*",
    "rollup": "../../node_modules/rollup/bin/rollup -c ../../scripts/bundling/rollup.umd.js && ../../node_modules/rollup/bin/rollup -c ../../scripts/bundling/rollup.es.js",
    "build-standalone": "ENTRY=scripts/standalone.js DEST=./dist/polythene-react-standalone ../../node_modules/rollup/bin/rollup -c ../../scripts/bundling/rollup.browser.js",
    "build": "npm run clean && npm run rollup && npm run build-standalone"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "polythene-core-css": "^1.4.1",
    "polythene-core": "^1.4.1",
    "polythene-css": "^1.4.1",
    "polythene-react-base": "^1.4.1",
    "polythene-react-button-group": "^1.4.1",
    "polythene-react-button": "^1.4.1",
    "polythene-react-card": "^1.4.1",
    "polythene-react-checkbox": "^1.4.1",
    "polythene-react-dialog-pane": "^1.4.1",
    "polythene-react-dialog": "^1.4.1",
    "polythene-react-drawer": "^1.4.1",
    "polythene-react-fab": "^1.4.1",
    "polythene-react-icon-button": "^1.4.1",
    "polythene-react-icon": "^1.4.1",
    "polythene-react-ios-spinner": "^1.4.1",
    "polythene-react-list-tile": "^1.4.1",
    "polythene-react-list": "^1.4.1",
    "polythene-react-material-design-progress-spinner": "^1.4.1",
    "polythene-react-material-design-spinner": "^1.4.1",
    "polythene-react-menu": "^1.4.1",
    "polythene-react-notification": "^1.4.1",
    "polythene-react-radio-button": "^1.4.1",
    "polythene-react-radio-group": "^1.4.1",
    "polythene-react-raised-button": "^1.4.1",
    "polythene-react-ripple": "^1.4.1",
    "polythene-react-search": "^1.4.1",
    "polythene-react-shadow": "^1.4.1",
    "polythene-react-slider": "^1.4.1",
    "polythene-react-snackbar": "^1.4.1",
    "polythene-react-svg": "^1.4.1",
    "polythene-react-switch": "^1.4.1",
    "polythene-react-tabs": "^1.4.1",
    "polythene-react-textfield": "^1.4.1",
    "polythene-react-toolbar": "^1.4.1"
