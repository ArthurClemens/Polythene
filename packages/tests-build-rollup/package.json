{
  "name": "polythene-build-rollup",
  "version": "1.3.2",
  "description": "",
  "private": true,
  "main": "dist/js/index",
  "module": "dist/js/index.mjs",
  "scripts": {
    "clean": "rm -rf dist/js/*",
    "rollup": "DEPS=1 ../../node_modules/rollup/bin/rollup -c ../../scripts/bundling/rollup.umd-includes.js && ../../node_modules/rollup/bin/rollup -c ../../scripts/bundling/rollup.es.js",
    "build": "npm run clean && npm run rollup",
    "serve": "http-server -p 5000 dist"
  },
  "dependencies": {
    "mithril": "2.0.0-rc.3",
    "polythene-css": "^1.3.2",
    "polythene-mithril": "^1.3.2"
  },
  "devDependencies": {
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-watch": "4.3.1",
    "rollup-webpack-loader": "^1.0.0"
  }
}
