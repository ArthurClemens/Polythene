{
  "name": "polythene-build-rollup",
  "version": "1.3.3",
  "description": "",
  "private": true,
  "main": "dist/js/index",
  "module": "dist/js/index.mjs",
  "scripts": {
    "clean": "rm -rf dist/js/*",
    "rollup": "../../node_modules/rollup/bin/rollup -c ../../scripts/bundling/rollup.umd-includes.js && ../../node_modules/rollup/bin/rollup -c ../../scripts/bundling/rollup.es.js",
    "build": "npm run clean && npm run rollup",
    "serve": "http-server -p 5000 dist"
  },
  "dependencies": {
    "mithril": "1.1.6",
    "polythene-css": "^1.3.3",
    "polythene-mithril": "^1.3.3"
  }
}
